{
  "name": "inferno-error-overlay",
  "version": "4.1.0",
  "description": "An overlay for displaying stack frames.",
  "main": "lib/index.js",
  "scripts": {
    "prepublishOnly": "npm run build:prod && npm test",
    "start": "cross-env NODE_ENV=development node build.js --watch",
    "test": "cross-env NODE_ENV=test jest",
    "build": "cross-env NODE_ENV=development node build.js",
    "build:prod": "cross-env NODE_ENV=production node build.js"
  },
  "repository": "facebookincubator/create-inferno-app",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/facebookincubator/create-inferno-app/issues"
  },
  "keywords": [
    "overlay",
    "syntax",
    "error",
    "red",
    "box",
    "redbox",
    "crash",
    "warning"
  ],
  "author": "Joe Haddad <timer150@gmail.com>",
  "files": [
    "lib/index.js"
  ],
  "devDependencies": {
    "anser": "1.4.6",
    "babel-code-frame": "6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-inferno-app": "^7.0.0",
    "babel-runtime": "6.26.0",
    "chalk": "^2.3.2",
    "chokidar": "^2.0.2",
    "cross-env": "5.1.4",
    "eslint": "4.19.0",
    "eslint-config-inferno-app": "^6.1.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-inferno": "^7.7.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "flow-bin": "^0.68.0",
    "html-entities": "1.2.1",
    "inferno": "^5",
    "inferno-vnode-flags": "^5",
    "jest": "22.4.2",
    "jest-fetch-mock": "1.4.2",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "settle-promise": "1.0.0",
    "source-map": "0.7.2",
    "webpack": "^3"
  },
  "jest": {
    "setupFiles": [
      "./src/__tests__/setupJest.js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "setupJest.js"
    ]
  }
}
